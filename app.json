{
  "expo": {
    "name": "KetolabV2",
    "slug": "mise-app",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "miseapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sinelicadigital.ketolab",
      "infoPlist": {
        "NSHealthShareUsageDescription": "KetoLab necessita di accedere ai dati sanitari per calcolare il metabolismo.",
        "NSHealthUpdateUsageDescription": "KetoLab necessita di aggiornare i dati sanitari.",
        "NSLocationWhenInUseUsageDescription": "KetoLab usa la posizione per meteo e consigli idratazione/sodio."
      }
    },
    "android": {
      "package": "com.tuonome.ketolabv2",
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACTIVITY_RECOGNITION",
        "android.permission.health.READ_STEPS",
        "android.permission.health.READ_ACTIVE_CALORIES_BURNED",
        "android.permission.health.READ_TOTAL_CALORIES_BURNED",
        "android.permission.health.READ_EXERCISE",
        "android.permission.health.READ_WEIGHT",
        "android.permission.health.READ_BODY_FAT",
        "android.permission.health.READ_HEART_RATE",
        "android.permission.health.READ_SLEEP",
        "android.permission.health.WRITE_STEPS",
        "android.permission.health.WRITE_WEIGHT",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-speech-recognition",
      "expo-router",
      "react-native-health-connect",
      "./plugins/withHealthConnectRules",
      "./plugins/withDisableMinification",
      "./plugins/withHealthConnectManifest","./plugins/withHealthConnectMainActivity",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F7F9FC",
          "dark": {
            "backgroundColor": "#F7F9FC"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 26,
            "kotlinVersion": "1.9.25",
            "enableProguardInReleaseBuilds": false,
            "enableShrinkResourcesInReleaseBuilds": false,
            "packagingOptions": {
              "pickFirst": [
                "**/libcrypto.so"
              ]
            }
          }
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d526e4b5-89a7-4e95-a758-75ac63d0b447"
      }
    }
  }
}