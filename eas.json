{
  "cli": {
    "version": ">= 16.31.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "Ketolabdebug": {
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "apk", "image": "latest" },
      "env": { "APP_DISPLAY_NAME": "Ketolabdebug" }
    },
    "preview": {
      "android": { "buildType": "apk", "image": "latest" },
      "distribution": "internal",
      "env": { "NODE_ENV": "production" }
    },
    "production": {
      "android": { "image": "latest" },
      "autoIncrement": true,
      "env": { "NODE_ENV": "production" }
    }
  },
  "submit": {
    "production": {}
  }
}
