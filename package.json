{
  "name": "mise-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:android": "node scripts/clear-expo-devices.js && expo start --android",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "android:dev": "node scripts/run-android-dev.js",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "build:apk": "eas build --platform android --profile preview --non-interactive",
    "build:apk:local": "cd android && .\\gradlew.bat assembleRelease",
    "build:apk:debug": "cd android && .\\gradlew.bat assembleDebug",
    "install:apk": "cd android && .\\gradlew.bat installRelease",
    "install:apk:debug": "cd android && .\\gradlew.bat installDebug"
  },
  "dependencies": {
    "@expo/vector-icons": "~14.0.4",
