    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@supabase/supabase-js": "^2.93.3",
    "date-fns": "^4.1.0",
    "expo": "~52.0.33",
    "expo-build-properties": "~0.13.3",
    "expo-constants": "~17.0.3",
    "expo-dev-client": "~5.0.20",
